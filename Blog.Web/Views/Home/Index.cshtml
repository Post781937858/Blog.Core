            @foreach (var item in Model)
            {
                <article id="post-110" class="wow fadeInUp post-110 post type-post status-publish format-standard hentry category-java tag-spring tag-upload rvy" data-wow-delay="0.3s">

                    <figure class="thumbnail">
                        <a href="@Url.Action("detail", "article", new { id = item.ArticleID })"><img src="~/image/@item.CoverURL" alt="Spring MVC  图片上传及预览"></a><span class="cat"><a href="http://java/">.NET开发</a></span>
                    </figure>

                    <header class="entry-header">
                        <h2 class="entry-title"><a href="@Url.Action("detail", "article", new { id = item.ArticleID })" rel="bookmark">@item.Title</a></h2>
                    </header><!-- .entry-header -->

                    <div class="entry-content">
                        <div class="archive-content">
                            @item.Synopsis
                        </div>
                        <span class="title-l"></span>

                        <span class="entry-meta">
                            <span class="date">2018年3月23日</span>
                            <span class="views"><i class="fa fa-eye"></i> 186</span>
                            <span class="views"><i class="fa fa-comment-o"></i> 152</span>

                            @*<span class="comment"><a href="javascript:" rel=""><i class="fa fa-comment-o"></i> 发表评论</a></span>*@@*<span class="favorite-s"><a class="wpzm-link" href="http://page/4/?wpzmaction=add&amp;postid=110" title="收藏本文" rel="nofollow"> <i class="fa fa-heart-o"></i> <span>收藏</span></a></span>*@
                        </span>

                        <div class="clear"></div>
                    </div><!-- .entry-content -->

                    <span class="entry-more"><a href="@Url.Action("detail", "article", new { id = item.ArticleID })" rel="bookmark">阅读全文</a></span>
                </article><!-- #post -->
            }
        </main><!-- .site-main -->

        <nav id="nav-below">
            <div class="nav-next"><a href="http://page/3/"></a></div>
            <div class="nav-previous"></div>
        </nav>

        <nav class="navigation pagination" role="navigation">
            <h2 class="screen-reader-text">文章导航</h2>
            <div class="nav-links">
                <a class="prev page-numbers" href="@(ViewBag.id!=1?(Url.Action("Index","Home",new { id=ViewBag.id-1})):(Url.Action("Index","Home",new { id=1})))"><i class="fa fa-angle-left"></i></a>
                @for (int i = 1; i <= ViewBag.Count; i++)
                {
                    if (ViewBag.id == i)
                    {
                        <span aria-current="page" class="page-numbers current"><span class="screen-reader-text">第 </span>@i<span class="screen-reader-text"> 页</span></span>
                    }
                    else
                    {
                        <a class="page-numbers" href="@Url.Action("Index","Home",new { id=i})"><span class="screen-reader-text">第 </span>@i<span class="screen-reader-text"> 页</span></a>
                    }
                }
               <a class="next page-numbers" href="@(ViewBag.id!=ViewBag.Count?(Url.Action("Index","Home",new { id=ViewBag.id+1})):(Url.Action("Index","Home",new { id=ViewBag.Count})))"><i class="fa fa-angle-right"></i></a>
            </div>
        </nav>
